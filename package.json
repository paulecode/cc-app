        "lint": "next lint",
        "test": "vitest",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "postinstall": "prisma generate",
        "db:create": "docker run --name postgres -p 5432:5432 -e POSTGRES_PASSWORD=password -d postgres; yarn prisma migrate dev --force",
        "db:stop": "docker stop postgres",
        "db:rm": "docker rm postgres",
        "testdb:create": "docker run --name test_postgres -p 5432:5432 -e POSTGRES_PASSWORD=password -d postgres; yarn prisma migrate reset --force",
        "testdb:stop": "docker stop test_postgres",
        "testdb:rm": "docker rm test_postgres"
    },
